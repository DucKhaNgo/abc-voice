<!--Modal: modalPush-->
<div class="modal fade  {{#if message}}show{{/if}}" id="modalPush" tabindex="-1" role="dialog"
    aria-labelledby="exampleModalLabel" aria-hidden="true" {{#if message}} style="display:block" {{/if}}>
    <div class="modal-dialog modal-notify modal-info" role="document">
        <!--Content-->
        <div class="modal-content text-center">
            <!--Header-->
            <div class="modal-header d-flex justify-content-center">
                <p class="heading">Message</p>
            </div>
            <!--Body-->
            <div class="modal-body">
                <i class="fas fa-bell fa-4x animated rotateIn mb-4"></i>
                <p>{{message}}</p>
            </div>
            <!--Footer-->
            <div class="modal-footer flex-center">
                <button type="button" class="btn btn_1"><a href="packages/verify" style="color:white">Yes</a></button>
                <button type="button" class="closeModalBtn btn btn_1" data-dismiss="modal">Close</button>
            </div>
        </div>
        <!--/.Content-->
    </div>
</div>
<!--Modal: modalPush-->


{{>header user=this.user}}
<!-- banner part start-->
{{>header user=this.user}}

<section class="breadcrumb breadcrumb_bg">
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <div class="breadcrumb_iner text-center">
                    <div style="height:100px" class="breadcrumb_iner_item">
                        <h2>Packages Price</h2>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- breadcrumb start-->

<!-- pricing part start-->
<section class="pricing_part section_padding single_page_pricing">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-6">
                <div class="section_tittle text-center">
                    <h2>Simple Pricing</h2>
                </div>
            </div>
        </div>
        <div class="row justify-content-center">
            {{#each  listKey}}
            <div class="col-lg-4 col-sm-6 mb-4">
                <div class="single_pricing_part">
                    <img src="img/icon/feature_icon_3.png" alt="">
                    <p>{{name}}</p>
                    <h3>{{price}} VND<span>/{{term}} days</span></h3>
                    <ul>
                        <li>{{term}} days</li>
                        <li>{{limitNumberOfReq}} Request Limit</li>
                        <li>24/7 Support</li>
                    </ul>
                    <a href="/packages/buy/{{id}}" class="btn_1">Buy</a>
                </div>
            </div>
            {{/each}}
        </div>
    </div>
    <img src="img/left_sharp.png" alt="" class="left_shape_1">
    <img src="img/animate_icon/Shape-1.png" alt="" class="feature_icon_1">
    <img src="img/animate_icon/shape.png" alt="" class="feature_icon_4">
</section>
<!-- pricing part end-->


{{>footer}}