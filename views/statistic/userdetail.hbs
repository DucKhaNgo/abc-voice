<section class="use_sasu">
    <div class="container">
        <div class="row justify-content-center">
            <div class="col-lg-12">
                <div class="section_tittle text-center">
                    <h2>USER DETAIL</h2>
                    <div class="section-top-border">
                        <h3 class="mb-30">User's Keys</h3>

                        <div class="progress-table-wrap">
                            <div class="progress-table">
                                <div class="table-head">
                                    <div class="serial">#</div>
                                    <div class="percentage">API Key</div>
                                    <div class="country">Package Type</div>
                                    <div class="country">Expired</div>
                                    <div class="visit">Change</div>
                                    <div class="visit">Renew</div>
                                    <div class="visit">Valid</div>
                                </div>

                                <div class="table-row">

                                    <div class="serial">{{id}}</div>
                                    <div class="percentage"><input form="updateform" type="hidden" name="id"
                                            value={{value}}>{{value}}</div>
                                    <input form="renewkey" type="hidden" name="idPackage" value={{id_package}}>
                                    <input form="renewkey" type="hidden" name="idKey" value={{id}}>

                                    <div class="country" style="justify-content: center">{{name_package}}</div>
                                    <div class="country" style="justify-content: center">{{date_expired}}</div>

                                    <div class="visit">
                                        <form id="updateform" method="POST" action='profile/updatekey'
                                            style="width:100%">
                                            <button style="justify-content: center"
                                                class="genric-btn primary-border circle" type="submit">Change</button>
                                        </form>

                                    </div>

                                    <div class="visit">
                                        <form id="renewkey" method="POST" action="profile/renewkey" style="width:100%">
                                            <button style="justify-content: center"
                                                class="genric-btn success-border circle" type="submit">Renew</button>
                                        </form>
                                    </div>
                                    {{!-- valid thì button là disable invalid thì enable --}}
                                    <div class="visit">
                                        <form id="renewkey" method="POST" action="profile/renewkey" style="width:100%">
                                            <button style="justify-content: center"
                                                class="genric-btn success-border circle" type="submit">Disable</button>
                                        </form>
                                    </div>

                                </div>

                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
